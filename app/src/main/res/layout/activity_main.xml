<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.hmartinez.mismapas.MainActivity">

        <ImageView
            android:id="@+id/ivFundidora_mainActivity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/parque_fundidora"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="67dp"
            android:layout_alignParentTop="true"
            android:layout_alignEnd="@+id/ivEstadioUANL_mainActivity"
            android:layout_marginEnd="27dp" />
        <ImageView
            android:id="@+id/ivEstadioBBVA_mainActivity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/estadio_bbva"
            android:layout_marginEnd="19dp"
            android:layout_marginTop="20dp"
            android:layout_alignTop="@+id/ivFundidora_mainActivity"
            android:layout_alignParentEnd="true" />
    <ImageView
        android:id="@+id/ivEstadioUANL_mainActivity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/estadio_universitario"
        android:layout_marginTop="27dp"
        android:layout_below="@+id/ivFundidora_mainActivity"
        android:layout_toStartOf="@+id/ivArenaMty_mainActivity"
        android:layout_marginEnd="11dp" />
    <ImageView
        android:id="@+id/ivArenaMty_mainActivity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/arena_monterrey"
        android:layout_alignTop="@+id/ivEstadioUANL_mainActivity"
        android:layout_alignEnd="@+id/ivEstadioBBVA_mainActivity" />
</RelativeLayout>
